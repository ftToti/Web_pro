<!-- 見出し -->
<div>
	<% if params[:version] == 'entry' %>
		<h3>応募しているユーザー</h3>
		<h5><%= @request.title %></h5>
	<% elsif params[:version] == 'scout' %>
		<h3>スカウトしているユーザー</h3>
		<h5><%= @request.title %></h5>
	<% elsif params[:version] == 'participant' %>
		<h3>参加しているユーザー</h3>
		<h5><%= @request.title %></h5>
	<% elsif params[:version] == 'evaluation' %>
		<h3>参加したユーザー</h3>
		<h5><%= @request.title %></h5>
	<% else %>
		<h3>すべてのユーザー</h3>
	<% end %>
	<%= @users.count %>人のユーザーが見つかりました
</div>

<!-- <プロフィール一覧> -->
<div class="d-flex flex-wrap">
	<% @users.each do |user| %>
		<div style="width:50%;">
			<%= render 'users/profile_box', user: user, request: @request %>
		</div>
	<% end %>
</div>
